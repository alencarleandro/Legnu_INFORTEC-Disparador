package br.com.legnu_propagar.controller;

import java.io.IOException;

import javafx.application.Application;
import javafx.fxml.FXMLLoader;
import javafx.scene.Parent;
import javafx.scene.Scene;
import javafx.stage.Stage;
import javafx.application.Application;

public abstract class Principal extends Application {
	
	public void iniciar(Stage stage) throws IOException {
		Parent root = FXMLLoader.load(getClass().getResource("TelaIntroduxcao.fxml"));

		Scene scene = new Scene(root);
		
		stage.setScene(scene);
		stage.show();
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		launch(args);

	}

}
